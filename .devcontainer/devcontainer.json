{
  "name": "sumisan-dev",
  "dockerFile": "Dockerfile",
  "runArgs": ["--network=sumisan-net"],
  "postCreateCommand": "./setup_sumisan_dev.sh",
  "forwardPorts": [1434, 5000, 5001, 4200],
  "remoteEnv": {
    "ASPNETCORE_ENVIRONMENT": "Development"
  },
  "mounts": [
    "source=${localWorkspaceFolder}/.env.sumisan,target=/workspaces/sumisan/.env.sumisan,type=bind,consistency=cached"
  ],
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:1": {
      "version": "latest"
    }
  }
}
